<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">



    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/send_basic_push"
        android:orientation="vertical"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:text="Send Basic Push Notification"
        android:gravity="center"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:lines="1"
        android:letterSpacing="0.1"
        android:background="@drawable/custom_btn"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_marginTop="20dp"
        android:onClick="sendPushNotification"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/send_custom_push"
        android:orientation="vertical"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:text="Send Image Push Notification"
        android:gravity="center"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:lines="1"
        android:letterSpacing="0.1"
        android:background="@drawable/custom_btn"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_marginTop="20dp"
        android:onClick="sendImagePushNotification"
        app:layout_constraintTop_toBottomOf="@id/send_basic_push"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/send_sound_push"
        android:orientation="vertical"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:text="Send Sound Push Notification"
        android:gravity="center"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:lines="1"
        android:letterSpacing="0.1"
        android:background="@drawable/custom_btn"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_marginTop="20dp"
        android:onClick="sendSoundPushNotification"
        app:layout_constraintTop_toBottomOf="@id/send_custom_push"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/send_large_push"
        android:orientation="vertical"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:text="Send Large Push Notification"
        android:gravity="center"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:lines="1"
        android:letterSpacing="0.1"
        android:background="@drawable/custom_btn"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_marginTop="20dp"
        android:onClick="sendLargePushNotification"
        app:layout_constraintTop_toBottomOf="@id/send_sound_push"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/send_badge_push"
        android:orientation="vertical"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:text="Send Badge Push Notification"
        android:gravity="center"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:lines="1"
        android:letterSpacing="0.1"
        android:background="@drawable/custom_btn"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_marginTop="20dp"
        android:onClick="sendLargePushNotification"
        app:layout_constraintTop_toBottomOf="@id/send_large_push"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>